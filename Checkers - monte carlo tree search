 = Checkers()\n","bigl = 'B'\n","\n","args = {\n","    'C': 1.41,\n","    'num_searches': 100000\n","}\n","\n","mcts = MCTS(checkers, args)\n","\n","state = [['---', 'c', '---', 'c', '---', '---', '---', 'c'],\n"," ['c', '---', '---', '---', 'c', '---', 'c', '---'],\n"," ['---', 'c', '---', 'b', '---', 'c', '---', 'c'],\n"," ['---', '---', '---', '---', '---', '---', '---', '---'],\n"," ['---', '---', '---', '---', '---', '---', '---', 'b'],\n"," ['---', '---', '---', '---', '---', '---', '---', '---'],\n"," ['---', '---', '---', 'b', '---', '---', '---', 'b'],\n"," ['b', '---', 'b', '---', 'b', '---', 'b', '---']]\n","\n","\n","while True:\n","  #bigl = B oznacza że rusza się teraz gracz, tutaj wszystko jest dość jasne\n","  #szukamy ruchów jakie może wykonać i wybieramy jeden z nich jako gracz.\n","    if bigl == 'B':\n","        valid_moves = checkers.find_player_available_moves(state, mandatory_jumping=False)\n","        #print('state where player is making a move')\n","        checkers.print_matrix(state)\n","        print(valid_moves)\n","        #print(\"valid_moves\", [i for i in range(tictactoe.action_size) if valid_moves[i] == 1])\n","        oldi = int(input('oldi :'))\n","        oldj = int(input('oldj :'))\n","        newi= int(input('newi :'))\n","        newj= int(input('newj :'))\n","\n","        #if valid_moves[action] == 0:\n","            #print(\"action not valid\")\n","            #continue\n","    else:\n","      #w tictactoe dostawialiśmy ze stanu [0,0,0][0,1,-1][1,0,0] stan [0,0,0][0,-1,1][-1,0,0] uzywajac\n","      #change perspective\n","        #print('state where computer is making a move')\n","        checkers.print_matrix(state)\n","        #neutral_state_1 = checkers.change_perspective(False, state)\n","        #neutral_state = checkers.change_pawn(False, neutral_state_1)\n","        #print('a')\n","        mcts_probs = mcts.search(state, 'B')\n","        print(mcts_probs)\n","        action = max(mcts_probs, key=mcts_probs.get)\n","  